<lattest>
  <gen>
    <lcg>
      <modulo basis="2" exponent="128"/>
      <order k="1"/>
      <period check="true"/>
      <coeff x="47026247687942121848144207491837418733"/>
    </lcg>
  </gen>
  <spectral>
    <reduction method="FULL"/>
    <norma lizer="BESTLAT"/>
    <dual bool="true"/>
  </spectral>
  <proj>
    <min x="2"/>
    <num x="1"/>
    <dim x="35"/>
  </proj>
</lattest>
