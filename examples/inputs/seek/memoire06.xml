<!--Combining 64 bits LCG and a MWC with smaller modulo. Hope the equivalent LCG
has modulo close to 2^64 so that we obtain a potent combination-->
<!--This works but not in higher dimensions-->
<seek>
  <gen>
    <numcomp x="2"/>
    <mrg>
      <modulo basis="2" exponent="63"/>
      <order x="1"/>
      <method random="1"/>
      <period check="true"/>
    </mrg>
    <mrg>
      <modulo basis="2" exponent="63" rest="-25"/>
      <method random="1"/>
      <period check="true"/>
    </mrg>
  </gen>
  <spectral>
    <reduction method="FULL"/>
    <norma x="BESTLAT"/>
    <dual x="true"/>
  </spectral>
  <proj>
    <min x="2"/>
    <num x="1"/>
    <dim x="34"/>
  </proj>
  <time x="60"/>
  <num_gen x="5"/>
</seek>
